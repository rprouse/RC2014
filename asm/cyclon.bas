REM Cyclon effect to I/O port LEDs on ports 0 and 1
4 LET DELAY = 50
5 PRINT "Push buttons 0..7 to change speed"
10 FOR F=1 TO 6
20 OUT 0,INT(2^F+.1)
25 OUT 1,INT(2^(7-F)+.1)
30 GOSUB 100
40 NEXT F
50 FOR F=7 TO 0 STEP -1
60 OUT 0,INT(2^F+.1)
65 OUT 1,INT(2^(7-F)+.1)
70 GOSUB 100
80 NEXT F
90 GOTO 10
100 FOR X=1 TO DELAY
110 IF INP(0) <> 0 THEN LET DELAY = INT(SQR(INP(0))) * 50
140 NEXT X
150 RETURN